<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body class="bodyStyle">
    <div class="aijia">AIJIA</div>
    <div class="wrapper">
        <nav id="sidebar">
            <div class="sidebar-header">
                AIJIA GESTION
            </div>
            <ul class="list-unstyled components">
               
                <li>
                    <a href="#menuAijia" data-toggle="collapse" aria-expanded="false" class="dropdown-toggle">Commandes</a>
                    <ul class="collapse list-unstyled" id="menuAijia">
                        <li>
                            <a href="#">Commandes</a>
                        </li>
                        <li>
                            <a href="#">Factures</a>
                        </li>
                        <li>
                            <a href="#">Livraisons</a>
                        </li>
                        <li>
                            <a href="#">MDL</a>
                        </li>
                    </ul>
                </li>
                <li>
                    <a href="#menuAijia2" data-toggle="collapse" aria-expanded="false" class="dropdown-toggle">Articles</a>
                    <ul class="collapse list-unstyled" id="menuAijia2">
                        <li>
                            <a href="#">Articles</a>
                        </li>
                        <li>
                            <a href="#">Marques</a>
                        </li>
                        <li>
                            <a href="#">Catégories</a>
                        </li>
                        <li>
                            <a href="#">Fournisseurs</a>
                        </li>
                    </ul>
                </li>
                <li>
                    <a href="#">Utilisateurs</a>
                </li>
                <li>
                    <a href="#">Messages</a>
                </li>
                <li>
                    <a href="#">Site</a>
                </li>
                <li>
                    <a href="#">Déconnexion</a>
                </li>
            </ul>
        </nav>
        <div id="content">
           
            
            <div>
                @RenderBody()
            </div>
            <div class="line">

            </div>

        </div>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
